        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Person settings

var mx = device_mouse_x_to_gui(0);
var my = device_mouse_y_to_gui(0);
var mj = device_mouse_check_button_pressed(0,mb_left);
var mk = device_mouse_check_button(0,mb_left);

yto = min(yto,DH-Chat_h-64-Chat_addy);

var dis = point_distance(x,y,xto,yto);
var dir = point_direction(x,y,xto,yto);
x += lengthdir_x(dis/10,dir);
y += lengthdir_y(dis/10,dir);

image_xscale += (sto-image_xscale)/10;
image_yscale = image_xscale;

Chat_addx += (sprite_width-sprite_xoffset-Chat_addx)/10;
Chat_addy += (-sprite_yoffset-Chat_addy)/10;
if iAct&lt;array_height_2d(bAct)&amp;&amp;is_string(bAct[iAct,0])
    Chat_w += (max(string_width(bAct[iAct,0]),string_width(Chat_txt))+33-Chat_w)/10;
else
    Chat_w += (string_width(Chat_txt)+33-Chat_w)/10;
Chat_w = min(Chat_w,Chat_wmax);

Chat_h += (string_height_ext(Chat_txt,20,Chat_w-32)+32-Chat_h)/10;

bAct_index++;
if bAct_pulse bAct_addrr = lengthdir_x(4,bAct_index*8);

Chat_click = 0;
Click = 0;

if mj&amp;&amp;point_in_rectangle(mx,my,x+Chat_addx,y+Chat_addy,x+Chat_addx+Chat_w,y+Chat_addy+Chat_h) Chat_press = 1;
if mj&amp;&amp;point_in_rectangle(mx,my,x-sprite_xoffset,y-sprite_yoffset,x-sprite_xoffset+sprite_width,y-sprite_yoffset+sprite_height) Press = 1;

if Chat_press {
    if !mk {Chat_press=0; Chat_click=1; PlaySom(sound1,0,VSom,0)}
    if !point_in_rectangle(mx,my,x+Chat_addx,y+Chat_addy,x+Chat_addx+Chat_w,y+Chat_addy+Chat_h) Chat_press = 0;
}

if Press {
    if !mk {Press=0; Click=1; PlaySom(sound1,0,VSom,0)}
    if !point_in_rectangle(mx,my,x-sprite_xoffset,y-sprite_yoffset,x-sprite_xoffset+sprite_width,y-sprite_yoffset+sprite_height) Press = 0;
}
</string>
