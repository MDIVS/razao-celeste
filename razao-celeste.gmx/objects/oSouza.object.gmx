if iAct&gt;=array_height_2d(bAct)||!is_string(bAct[iAct,0]) {bAct_alpha += -bAct_alpha/10; exit};
bAct_alpha += (1-bAct_alpha)/10;

var mj = device_mouse_check_button_pressed(0,mb_left);

if mj {
    var h = string_height_ext(bAct[iAct,0],20,Chat_w-32)+32;
    var x1=x+Chat_addx, y1=y+Chat_addy+Chat_h+8, x2=x+Chat_addx+Chat_w, y2=y+Chat_addy+Chat_h+8+h;
    var mx = device_mouse_x_to_gui(0);
    var my = device_mouse_y_to_gui(0);
    if point_in_rectangle(mx,my,x1,y1,x2,y2) {
        if is_string(bAct[iAct,1]) {
            Chat_txt = bAct[iAct,1];
            Chat_count = 0;
        } else if script_exists(bAct[iAct,1]) script_execute(bAct[iAct,1]);
        PlaySom(sound1,0,VSom,0);
        iAct++;
    }
}
</string>
